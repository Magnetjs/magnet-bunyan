{
  "name": "magnet-bunyan",
  "version": "2.0.0",
  "description": "Magnet adapter for Bunyan",
  "keywords": [
    "bunyan",
    "log",
    "magnet",
    "magnetjs"
  ],
  "homepage": "https://github.com/Magnetjs/magnet-bunyan#readme",
  "bugs": {
    "url": "https://github.com/Magnetjs/magnet-bunyan/issues"
  },
  "license": "MIT",
  "author": "Kieve Chua <kievechua@gmail.com> (https://github.com/kievechua)",
  "files": [
    "README",
    "CHANGELOG.md",
    "dist"
  ],
  "main": "dist/index.js",
  "repository": "magnetjs/magnet-bunyan",
  "scripts": {
    "start": "fly",
    "build": "fly build",
    "dist": "npm run build && cd dist && npm publish",
    "test": "ava | tap-spec",
    "watch": "tsc -w",
    "watch-all": "concurrently \"npm run watch\" \"npm run watch-test\"",
    "watch-test": "ava --watch | tap-spec",
    "prepublish": "npm run snyk-protect",
    "snyk-protect": "snyk-protect"
  },
  "dependencies": {
    "bunyan": "^1.8.13",
    "magnet-core": "^4.0.1",
    "@snyk/protect": "latest"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "fly": "^2.0.6",
    "fly-clear": "^1.0.1",
    "fly-esnext": "^2.0.1",
    "fly-flatten": "^1.0.1",
    "fly-typescript": "^1.1.0",
    "fly-watch": "^1.1.1"
  },
  "engines": {
    "node": ">=6.9.5"
  },
  "ava": {
    "failFast": true,
    "tap": true,
    "source": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ]
  },
  "snyk": true
}
