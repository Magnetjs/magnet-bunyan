{
  "name": "magnet-bunyan",
  "version": "0.1.1",
  "description": "Magnet adapter for Bunyan",
  "main": "dist/index.js",
  "scripts": {
    "start": "gulp watch",
    "prepublish": "babel src --presets babel-preset-es2015,babel-preset-stage-0 --out-dir dist",
    "build": "babel src --presets babel-preset-es2015,babel-preset-stage-0 --out-dir dist",
    "test": "babel-node --presets es2015,stage-0 tests/index.js | tap-spec",
    "watch-test": "nodemon --exec babel-node --presets es2015,stage-0 tests/index.js | tap-spec"
  },
  "keywords": [
    "magnet",
    "log",
    "bunyan"
  ],
  "author": "Kieve Chua <kievechua@gmail.com>",
  "license": "MIT",
  "files": [
    "README",
    "CHANGELOG.md",
    "dist"
  ],
  "dependencies": {
    "bunyan": "^1.7.1",
    "magnet-core": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-async-to-generator": "^6.7.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-tape-runner": "^2.0.1",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-import": "^1.1.0",
    "eslint": "^2.4.0",
    "gulp-babel": "^6.1.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp": "^3.9.1",
    "nodemon": "^1.9.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Magnetjs/magnet-bunyan.git"
  },
  "bugs": {
    "url": "https://github.com/Magnetjs/magnet-bunyan/issues"
  },
  "homepage": "https://github.com/Magnetjs/magnet-bunyan#readme"
}
