{
  "name": "magnet-bunyan",
  "version": "1.2.1",
  "description": "Magnet adapter for Bunyan",
  "keywords": [
    "bunyan",
    "log",
    "magnet",
    "magnetjs"
  ],
  "homepage": "https://github.com/Magnetjs/magnet-bunyan#readme",
  "bugs": {
    "url": "https://github.com/Magnetjs/magnet-bunyan/issues"
  },
  "license": "MIT",
  "author": "Kieve Chua <kievechua@gmail.com> (https://github.com/kievechua)",
  "files": [
    "README",
    "CHANGELOG.md",
    "dist"
  ],
  "main": "dist/index.js",
  "repository": "magnetjs/magnet-bunyan",
  "scripts": {
    "build": "tsc",
    "dist": "npm run build && npm publish",
    "start": "tsc",
    "test": "ava | tap-spec",
    "watch": "tsc -w",
    "watch-all": "concurrently \"npm run watch\" \"npm run watch-test\"",
    "watch-test": "ava --watch | tap-spec"
  },
  "dependencies": {
    "bunyan": "^1.7.1",
    "magnet-core": "^3.6.1"
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "concurrently": "^3.3.0",
    "fast-async": "^6.2.0",
    "nodemon": "^1.9.1",
    "standard": "^8.6.0",
    "tap-spec": "^4.1.1"
  },
  "engines": {
    "node": ">=6.9.5"
  },
  "ava": {
    "failFast": true,
    "tap": true,
    "source": [
      "**/*.{js,jsx}",
      "!dist/**/*"
    ]
  }
}
